{
  "expo": {
    "version": "2.0.8",
    "name": "الصولجان",
    "scheme": "alsolajan-app",
    "description": "الصولجان منصة تتيح للأفراد عرض وبيع منتجاتهم عبر الإنترنت دون الحاجة لشرائها أو تخزينها. يكفي ملء نموذج طلب البيع واستلام الأرباح مباشرة من الفرق بين سعر الجملة وسعر البيع.",
    "slug": "alsolajan-app",
    "privacy": "public",
    "androidStatusBar": {
      "backgroundColor": "#FFFFFF"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.solajan.tstore",
      "usesAppleSignIn": true
    },
    "android": {
      "versionCode": 9,
      "package": "com.solajan.tstore",
      "permissions": ["INTERNET"],
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "iosDisplayInForeground": true,
      "color": "#D0AD8A",
      "icon": "./assets/icon.png",
      "androidMode": "default",
      "androidCollapsedTitle": "الصولجان"
    },
    "extra": {
      "eas": {
        "projectId": "0b40e789-4966-41e5-86c4-a1d7fe586e74"
      }
    }
  }
}
